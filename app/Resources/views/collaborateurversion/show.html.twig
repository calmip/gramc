{% extends 'base.html.twig' %}

{% block body %}
    <h1>Collaborateurversion</h1>

    <table>
        <tbody>
            <tr>
                <th>Responsable</th>
                <td>{% if collaborateurVersion.responsable %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Login</th>
                <td>{% if collaborateurVersion.login %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ collaborateurVersion.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('collaborateurversion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('collaborateurversion_edit', { 'id': collaborateurVersion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
